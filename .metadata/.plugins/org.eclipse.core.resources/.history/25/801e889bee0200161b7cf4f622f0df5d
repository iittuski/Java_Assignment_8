package j8;

import java.util.Scanner;

public class j8 {
	public static void main(String args[])
	{
		Scanner s = new Scanner(System.in);
		System.out.println("Enter The month index \n 1-Jan \n 2-Feb \n 3-Mar \n 4-Apr \n 5-May \n 6-Jun \n 7-July \n 8-Aug \n 9-Sep \n 10-Oct \n 11-Nov \n 12-dec");
		int i = s.nextInt();
		switch(i){
		case 1:
			System.out.println("Total No of Days are 30");
		case 2:
			System.out.println("Total No of Days are 28");
		case 3:
			System.out.println("Total No of Days are 30");
		case 4:
			System.out.println("Total No of Days are 31");
		case 5:
			System.out.println("Total No of Days are 30");
		case 6:
			System.out.println("Total No of Days are 31");
		case 7:
			System.out.println("Total No of Days are 30");
		case 8:
			System.out.println("Total No of Days are 31");
		case 9:
			System.out.println("Total No of Days are 30");
		case 10:
			System.out.println("Total No of Days are 31");
		case 11:
			System.out.println("Total No of Days are 30");
		case 12:
			System.out.println("Total No of Days are 31");
			
		}
		
	}

}
